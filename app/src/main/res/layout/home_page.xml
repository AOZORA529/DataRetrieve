<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_queryAndAggregateUsageStats"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="queryAndAggregateUsageStats" />

    <Button
        android:id="@+id/btn_queryUsageStats"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="queryUsageStats" />

    <Button
        android:id="@+id/btn_queryEvents"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="queryevents" />

    <Button
        android:id="@+id/btn_queryEventRow"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="queryeventrow" />

    <Button
        android:id="@+id/btn_create_table"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="create table" />

    <Button
        android:id="@+id/btn_get_icons"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="GET ICONS" />
</LinearLayout>